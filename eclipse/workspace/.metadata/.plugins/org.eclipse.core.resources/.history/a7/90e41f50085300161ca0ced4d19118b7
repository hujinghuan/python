#!/usr/bin/env python
# -*- coding: utf-8 -*-

import pandas as pd
import numpy as np
from scipy import  stats
import matplotlib.pyplot as plt
import statsmodels.api as sm

prices=pd.read_csv('prices.csv',index=['date'],dtype='float64')
volume=pd.read_csv('volume.csv',index=['date'],dtype='float64')
total=prices * volume
print(total)