#!/usr/bin/env python
# -*- coding: utf-8 -*-

import pandas as pd
import numpy as np
from scipy import  stats
import matplotlib.pyplot as plt
import statsmodels.api as sm

prices=pd.read_csv('prices.csv',dtype='float64',index=['date'])
volume=pd.read_csv('volume.csv',dtype='float64',index=['date'])
total=prices * volume
print(total)